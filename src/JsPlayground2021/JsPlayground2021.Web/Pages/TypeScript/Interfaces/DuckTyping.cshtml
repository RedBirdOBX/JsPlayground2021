@page

@model JsPlayground.Pages.TypeScript.Interfaces.DuckTypingModel

@{
    string pageId = "Duck Typing";
    Layout = "~/Pages/Shared/_PageLayout.cshtml";
}

@{
    ViewData["Title"] = pageId;
}

@section LibraryHeader {TypeScript}
@section PageName {@pageId}

@section Menu
{
    <partial name="_TypeScriptMenu" />
}

<h5 class="my-3 text-info text-center">
    Tip: Interfaces cannot be directly instantiated. They can however, used to define a Type.
</h5>

<p>
    "duck typing" is sometimes used to describe when one object is defined as another type who's shape just happens to match.
</p>

<pre>
<code>
interface Employee
{
    Name: string;
    Title: string;
}

let Developer =
{
    Name: "Ricky Bobby",
    Title: "Senior Developer",
    Editor: "Visual Studio Code"
}

let newEmployee: Employee = Developer;
    </code>
</pre>

<p>&nbsp;</p>


