@page

@using JsPlayground2021.Web.Data.Enums
@using JsPlayground.Pages.Javascript.Basics

@{
    Layout = "~/Pages/Shared/_PageLayout.cshtml";
}

@{
    ViewData["Title"] = "VS Code";
}

@section LibraryHeader {JavaScript}
@section PageName {VS Code}

@section Menu
{
    <partial name="_MenuContainer" model="MenuType.Javascipt" />
}
<p>
    VS Code is our favorite IDE (so far) for front-end development. Make sure you have these helpful
    extensions installed.
</p>

<ul>
    <li>Code Spell Checker</li>
    <li>ESLint</li>
    <li>HTML Snippets</li>
</ul>

<p class="text-center"><img src="../../assets/images/extensions.png" /></p>

<p>&nbsp;</p>

<p>&nbsp;</p>

@section Scripts
{
    <script>
        var fruits = ["Banana", "Orange", "Apple", "Mango"];
        document.getElementById("example1").innerHTML = fruits;

        var fruits = ["Banana", "Orange", "Apple", "Mango"];
        document.getElementById("example2").innerHTML = fruits.valueOf();

        var fruits = ["Banana", "Orange", "Apple", "Mango"];
        document.getElementById("example3").innerHTML = fruits.toString();
    </script>

}
